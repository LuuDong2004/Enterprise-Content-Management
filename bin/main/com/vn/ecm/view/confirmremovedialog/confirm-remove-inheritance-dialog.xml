<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<view xmlns="http://jmix.io/schema/flowui/view"
      title="Xác nhận xóa quyền kế thừa">
    <layout>
        <vbox spacing="true" padding="true" width="100%">
            <span id="messageSpan" text="Bạn có chắc chắn muốn xóa hoàn toàn tất cả các quyền kế thừa?"/>
            
            <hbox spacing="true" alignSelf="END">
                <button id="noBtn" text="Không"/>
                <button id="yesBtn" text="Có" themeNames="error"/>
            </hbox>
        </vbox>
    </layout>
</view>

