<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="ECM">
        <dropForeignKeyConstraint baseTableName="FILE" constraintName="FK_FILE_ON_FOLDER"/>
    </changeSet>
    <changeSet id="2" author="ECM">
        <dropForeignKeyConstraint baseTableName="FOLDER" constraintName="FK_FOLDER_ON_PARENT"/>
    </changeSet>
    <changeSet id="3" author="ECM">
        <dropTable cascadeConstraints="true" tableName="FILE"/>
    </changeSet>
    <changeSet id="4" author="ECM">
        <dropTable cascadeConstraints="true" tableName="FOLDER"/>
    </changeSet>
</databaseChangeLog>